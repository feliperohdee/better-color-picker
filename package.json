{
  "name": "color-picker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
	"build": "tsc && vite build",
    "build-deploy-storybook": "storybook build --output-dir ./dist-storybook && aws s3 sync dist-storybook s3://feliperohde/color-picker-storybook --cache-control no-cache --acl public-read --delete",
    "build-deploy-demo": "yarn build && aws s3 sync dist s3://feliperohde/color-picker --cache-control no-cache --acl public-read --delete",
    "dev": "vite",
    "dev-storybook": "storybook dev -p 6006",
    "export": "yarn build --outDir ./dist-export --mode export",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "chroma-js": "^2.4.2",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "tailwind-colors-generator": "https://github.com/feliperohdee/tailwind-colors-generator"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/blocks": "^8.1.5",
    "@storybook/react": "^8.1.5",
    "@storybook/react-vite": "^8.1.5",
    "@storybook/test": "^8.1.5",
    "@types/chroma-js": "^2.4.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "storybook": "^8.1.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-optimize-css-modules": "^1.0.6",
    "vitest": "^1.6.0"
  }
}
